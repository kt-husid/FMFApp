@model IEnumerable<BootstrapWebApplication.Models.ChangeEvent>

@{
    ViewBag.Title = @Resources.ChangeEvents;
}

<h2>@BootstrapResources.Resources.ChangeEvents</h2>

@*@Html.Partial("_CreatePartial")*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreatedOn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeletedOn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedOn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedOn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeletedOn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedOn)
        </td>
        <td>
            @Html.Partial("_EditDetailsDeletePartial", item)
        </td>
    </tr>
}

</table>
