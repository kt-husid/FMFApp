            <li class="active"><a href="#">Overview</a></li>
            <li><a href="#">Some nav link</a></li>
        </ul>
    }*@
@{
    var pageData = new PageData()
    {
        Title = @Resources.Title,
        Titles = @Resources.Title
    };
}
@Html.Partial("_PagedListHeaderJSPartial", new PagedListHeaderData() { Title = pageData.Titles })
<div class="paged-area">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FirstOrDefault().IsPrimary)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstOrDefault().RegNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstOrDefault().AccountNumber)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody data-bind="foreach: List">
            <tr class="data">
                <td data-bind="text: IsPrimary"></td>
                <td data-bind="text: RegNumber"></td>
                <td data-bind="text: AccountNumber"></td>
                <td class="data" style="width:250px; text-align:left;">
                    @Html.Partial("_EditDetailsDeletePartial2", new EditDetailsDelete2Data() { Title = @pageData.Title })
                </td>
            </tr>
        </tbody>
    </table>
</div>
@Html.Partial("_PagedListFooterJSPartial", new PagedListFooterData() { ViewModelUrl = "/api/BankAccount", PagedList = @Model })