@model BootstrapWebApplication.Models.MemberSignOnReportParameters
@{
    //ViewBag.Title = @Resources.ReportFor + " " + @Resources.Member + " " + @Resources.SignOns;
    Layout = "~/Views/Shared/_LayoutReporting.cshtml";
}
@Html.Partial("_ReportHeaderPartial")
@{
    var From = DateTimeHelper.ParseForReport(@Request.QueryString["From"], Core.Settings.Instance.DateTimeFormatReporting);
    var To = DateTimeHelper.ParseForReport(@Request.QueryString["To"], Core.Settings.Instance.DateTimeFormatReporting);
    var reportSource = new UriReportSource() { Uri = "MemberSignOnsReport.trdx" };
    reportSource.Parameters.Add("MemberId", @Request.QueryString["MemberId"]);
    reportSource.Parameters.Add("From", @From);
    reportSource.Parameters.Add("To", @To);
}
@Html.Partial("_ReportFooterPartial", reportSource)
<script>
    function onPrint() {
        // Optionally implement code to be executed after the print button on the Telerik Report Viewer has been clicked
    }
</script>