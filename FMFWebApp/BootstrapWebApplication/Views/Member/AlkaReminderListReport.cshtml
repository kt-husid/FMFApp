@model BootstrapWebApplication.Models.MemberSignOnReportParameters
@{
    //ViewBag.Title = @Resources.ReportFor + " " + @Resources.Member + " " + @Resources.SignOns;
    Layout = "~/Views/Shared/_LayoutReporting.cshtml";
}
@Html.Partial("_ReportHeaderPartial")
@{
    var SignOnTo = DateTimeHelper.ParseForReport(@Request.QueryString["SignOnTo"]);
    var reportSource = new UriReportSource() { Uri = "AlkaReminderList_p1798.trdx" };
    reportSource.Parameters.Add("FromPostalCode", @Request.QueryString["PostalCodeFrom"]);
    reportSource.Parameters.Add("ToPostalCode", @Request.QueryString["PostalCodeTo"]);
    reportSource.Parameters.Add("IncludeLessThan30Days", @Request.QueryString["IncludeLessThan30Days"]);
    reportSource.Parameters.Add("DaysUntilInsuranceExpires", @Request.QueryString["DaysUntilInsuranceExpires"]);
    reportSource.Parameters.Add("SignOnTo", @SignOnTo);
}
@Html.Partial("_ReportFooterPartial", reportSource)
<script>
