@model BootstrapWebApplication.Models.SaleStatsModel
@{
    //ViewBag.Title = @Resources.ReportFor + " " + @Resources.Member + " " + @Resources.SignOns;
    Layout = "~/Views/Shared/_LayoutReporting.cshtml";
}
@Html.Partial("_ReportHeaderPartial")
@{
    var From = DateTimeHelper.ParseForReport(@Request.QueryString["From"]);
    var To = DateTimeHelper.ParseForReport(@Request.QueryString["To"]);
    var reportSource = new UriReportSource() { Uri = "SaleStats_p2773.trdx" };
    reportSource.Parameters.Add("From", @From);
    reportSource.Parameters.Add("To", @To);
    reportSource.Parameters.Add("ShipTypeCode", @Request.QueryString["ShipTypeCode"]);
    reportSource.Parameters.Add("UserIdCode", @Model.UserIdCode);
}
@Html.Partial("_ReportFooterPartial", reportSource)
<script>
    function onPrint() {
        // Optionally implement code to be executed after the print button on the Telerik Report Viewer has been clicked
    }
</script>